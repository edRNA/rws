{
  "compilerOptions": {
    "baseUrl": "../",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "ES2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./vendors/rws",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "declaration": false, 
    "paths": {
      "rws-js-server": [
        "../src"
      ]
    }
  },
  "include": [
    "../src"
  ],
  "exclude": [
    "../../../node_modules"
  ]
}